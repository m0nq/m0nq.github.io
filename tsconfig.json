{
    "compilerOptions": {
        "target": "esnext",
        "lib": ["dom", "esnext"],
        "jsx": "react-jsx",
        "jsxImportSource": "theme-ui",
        "module": "esnext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "baseUrl": "./src",
        "paths": {
            "@/*": ["./src/*"]
        }
    },
    "include": [
        "./src/**/*",
        "./gatsby-node.ts",
        "./gatsby-config.ts",
        "./plugins/**/*"
    ]
}
